{
  "name": "dependencies-cli",
  "version": "1.0.0",
  "author": "Andrew Fawcett",
  "bugs": "https://github.com/afawcett/dependencies-cli/issues",
  "dependencies": {
    "@oclif/config": "^1.6.27",
    "@oclif/errors": "^1.0.9",
    "@salesforce/command": "0.1.6",
    "babel-preset-latest": "^6.24.1",
    "child-process": "^1.0.2",
    "child_process": "^1.0.2",
    "debug": "^3.1.0",
    "file-exists": "^5.0.1",
    "file-system": "^2.2.2",
    "shelljs": "^0.8.2",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.2.20",
    "@oclif/plugin-help": "^1.1.6",
    "@oclif/test": "^1.0.1",
    "@oclif/tslint": "^1.0.2",
    "@salesforce/dev-config": "^1.1.0",
    "@types/chai": "^4.1.2",
    "@types/jsforce": "^1.8.13",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.0.4",
    "sinon": "^6.0.0",
    "ts-node": "5",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib",
    "/messages"
  ],
  "homepage": "https://github.com/afawcett/dependencies-cli",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "afawcett/dependencies-cli",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepare": "yarn run build && oclif-dev manifest",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": {
    "componentizer": "./bin/run"
  }
}
